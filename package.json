{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:next": "TZ=Etc/UTC next dev --port 1337",
    "dev:graphql": "bun run codegen -- --watch",
    "build": "npm run build:graphql && npm run build:next",
    "build:next": "next build",
    "build:graphql": "bun run codegen",
    "start": "next start",
    "lint": "next lint",
    "dump-prod": "mongodump --excludeCollection=kilterboard_climbs  --excludeCollection=kilterboard_climb_stats --uri=\"$(npx --yes dotenv-cli -e .env.local -- bash -c 'echo $PROD_MONGODB_URL')\" --out=\"mongodump/$(date '+%Y-%m-%d-%H:%M:%S')\"",
    "restore-local-from-dump": "mongorestore -h 127.0.0.1 --port 27017 --drop --nsFrom=\"self.*\" --nsTo=\"test.*\"",
    "copy-prod": "npm run dump-prod && npm run restore-local-from-dump mongodump/$(ls -Art mongodump | tail -n 1)",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^4.1.4",
    "@apollo/client-integration-nextjs": "^0.14.4",
    "@apollo/server": "^5.4.0",
    "@as-integrations/next": "^4.1.0",
    "@auth/mongodb-adapter": "3.11.1",
    "@date-fns/tz": "^1.4.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@fortawesome/fontawesome-svg-core": "^7.2.0",
    "@fortawesome/free-regular-svg-icons": "^7.2.0",
    "@fortawesome/free-solid-svg-icons": "^7.2.0",
    "@fortawesome/react-fontawesome": "^3.2.0",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typed-document-node": "^6.1.6",
    "@graphql-codegen/typescript": "^5.0.8",
    "@graphql-codegen/typescript-resolvers": "^5.1.6",
    "@graphql-tools/schema": "^10.0.31",
    "@nivo/line": "^0.99.0",
    "@parcel/watcher": "^2.5.6",
    "@tailwindcss/postcss": "^4.2.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.6.1",
    "@vercel/functions": "^3.4.2",
    "ably": "^2.17.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "baseline-browser-mapping": "^2.9.19",
    "cheerio": "^1.2.0",
    "date-fns": "^4.1.0",
    "echarts-for-react": "^3.0.6",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react-hooks": "7.0.1",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "ical-generator": "^10.0.0",
    "luxon": "^3.7.2",
    "mongodb": "^7.1.0",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "npm-run-all": "^4.1.5",
    "partysocket": "1.1.13",
    "postcss": "^8.5.6",
    "react": "^19.2.4",
    "react-apollo-network-status": "^6.0.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-select": "^5.10.2",
    "remark-gfm": "^4.0.1",
    "rrule": "^2.8.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@serwist/turbopack": "^9.5.6",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "dayjs": "^1.11.19",
    "esbuild": "^0.27.3",
    "eslint": "^8.57.1",
    "partykit": "0.0.115",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "serwist": "^9.5.6",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "esbuild",
    "sharp",
    "workerd"
  ]
}
