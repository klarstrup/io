{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "dump-prod": "mongodump --excludeCollection=kilterboard_climbs  --excludeCollection=kilterboard_climb_stats --uri=\"$(npx --yes dotenv-cli -e .env.local -- bash -c 'echo $PROD_MONGODB_URL')\" --out=\"mongodump/$(date '+%Y-%m-%d-%H:%M:%S')\"",
    "restore-local-from-dump": "mongorestore -h 127.0.0.1 --port 27017 --drop --nsFrom=\"self.*\" --nsTo=\"test.*\"",
    "copy-prod": "npm run dump-prod && npm run restore-local-from-dump mongodump/$(ls -Art mongodump | tail -n 1)"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "3.10.0",
    "@date-fns/tz": "^1.4.1",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@nivo/line": "^0.99.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^3.1.1",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "echarts-for-react": "^3.0.2",
    "eslint-config-next": "^15.5.4",
    "eslint-plugin-react-hooks": "6.1.1",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "ical-generator": "^9.0.0",
    "luxon": "^3.7.2",
    "mongodb": "^6.20.0",
    "next": "^15.5.4",
    "next-auth": "^5.0.0-beta.29",
    "partysocket": "1.1.6",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-select": "^5.10.2",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.14",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "dayjs": "^1.11.18",
    "eslint": "^8.57.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.3",
    "partykit": "0.0.115"
  },
