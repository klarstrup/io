{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "dump-prod": "mongodump --uri=\"$(npx --yes dotenv-cli -e .env.local -- bash -c 'echo $PROD_MONGODB_URL')\" --out=\"mongodump/$(date '+%Y-%m-%d-%H:%M:%S')\"",
    "restore-local-from-dump": "mongorestore -h 127.0.0.1 --port 27017 --drop --nsFrom=\"self.*\" --nsTo=\"test.*\"",
    "copy-prod": "npm run dump-prod && npm run restore-local-from-dump mongodump/$(ls -Art mongodump | tail -n 1)"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "3.10.0",
    "@date-fns/tz": "^1.4.1",
    "@nivo/line": "^0.99.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^2.2.13",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "echarts-for-react": "^3.0.2",
    "eslint-config-next": "^15.5.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "ical-generator": "^9.0.0",
    "luxon": "^3.7.1",
    "mongodb": "^6.19.0",
    "next": "^15.5.2",
    "next-auth": "^5.0.0-beta.25",
    "postcss": "^8.5.6",
    "react": "npm:react@rc",
    "react-dom": "npm:react-dom@rc",
    "react-hook-form": "^7.62.0",
    "react-select": "^5.10.2",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.12",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.0",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "dayjs": "^1.11.18",
    "eslint": "^8.57.1",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "react": "npm:react@rc",
    "react-dom": "npm:react-dom@rc",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
