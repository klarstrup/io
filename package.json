{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dump-prod": "mongodump --uri=\"$(npx --yes dotenv-cli -e .env.local -- bash -c 'echo $PROD_MONGODB_URL')\" --out=\"mongodump/$(date '+%Y-%m-%d-%H:%M:%S')\"",
    "restore-local-from-dump": "mongorestore -h 127.0.0.1 --port 27017 --drop --nsFrom=\"self.*\" --nsTo=\"test.*\"",
    "copy-prod": "npm run dump-prod && npm run restore-local-from-dump mongodump/$(ls -Art mongodump | tail -n 1)"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "3.7.4",
    "@date-fns/tz": "^1.2.0",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.4.1",
    "@vercel/functions": "^1.5.2",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "eslint-config-next": "^15.1.2",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "ical-generator": "^8.0.1",
    "luxon": "^3.5.0",
    "mongodb": "^6.12.0",
    "next": "^15.1.2",
    "next-auth": "^5.0.0-beta.25",
    "postcss": "^8.4.49",
    "react": "npm:react@rc",
    "react-dom": "npm:react-dom@rc",
    "react-hook-form": "^7.54.2",
    "react-select": "^5.9.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.10.2",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "react": "npm:react@rc",
    "react-dom": "npm:react-dom@rc",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
}
