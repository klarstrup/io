{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dump-prod": "mongodump --uri=\"$(npx --yes dotenv-cli -e .env.local -- bash -c 'echo $PROD_MONGODB_URL')\" --out=\"mongodump/$(date '+%Y-%m-%d-%H:%M:%S')\"",
    "restore-local-from-dump": "mongorestore -h 127.0.0.1 --port 27017 --drop --nsFrom=\"self.*\" --nsTo=\"test.*\"",
    "copy-prod": "npm run dump-prod && npm run restore-local-from-dump mongodump/$(ls -Art mongodump | tail -n 1)"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "3.8.0",
    "@date-fns/tz": "^1.2.0",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^2.0.0",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "eslint-config-next": "^15.3.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "ical-generator": "^8.1.1",
    "luxon": "^3.6.1",
    "mongodb": "^6.15.0",
    "next": "^15.3.0",
    "next-auth": "^5.0.0-beta.25",
    "postcss": "^8.5.3",
    "react": "npm:react@rc",
    "react-dom": "npm:react-dom@rc",
    "react-hook-form": "^7.55.0",
    "react-select": "^5.10.1",
    "sharp": "^0.34.1",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.14.0",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "dayjs": "^1.11.13",
    "eslint": "^8.57.1",
    "eslint-plugin-react-compiler": "^19.0.0-beta-e993439-20250405",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "react": "npm:react@rc",
    "react-dom": "npm:react-dom@rc",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  }
